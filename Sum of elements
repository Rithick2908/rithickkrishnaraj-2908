#include<stdio.h>
void main()
{
    int size,sum,element;
    printf("Enter the size of integer array:");
    scanf("%d",&size);
    int arr[size];
    for(int i=0;i<size;i++)
    {
        printf("Enter element:");
        scanf("%d",&element);
        sum+=element;
        arr[i]=element;
    }
    printf("the sum is%d",sum);
}
